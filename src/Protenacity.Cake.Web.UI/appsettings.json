{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Global": {
        "Id": "4980cb41-e6ac-46ef-ad44-1c6fd9bd6700",
        "SanitizeTinyMce": true,
        "TimeOut": "24:00:00",
        "VersionCheckPeriod": 0,
        "Smtp": {
          "From": "webteam@protenacity.co.uk",
          "Host": "localhost",
          "Port": 25,
          "SecureSocketOptions": "Auto",
          "DeliveryMethod": "Network",
          "PickupDirectoryLocation": "",
          "Username": "webteam@protenacity.co.uk",
          "Password": "SuperSecretPassword"
        },
        "GraphEmail": {
          "TenantId": "MyTenantValue",
          "ClientId": "MyClientId",
          "ClientSecret": "Shhh",
          "FromEmailUser": "webteam@protenacity.co.uk"
        }
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "Protenacity.Cake.Web.Core.Constitution",
        "ModelsDirectory": "~/../Protenacity.Cake.Web.Core/Constitution/AutoGenerated",
        "AcceptUnsafeModelsDirectory": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "Unattended": {
        "UpgradeUnattended": true
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}